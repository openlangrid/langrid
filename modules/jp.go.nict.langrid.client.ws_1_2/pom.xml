<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <packaging>jar</packaging>
  <artifactId>jp.go.nict.langrid.client.ws_1_2</artifactId>
  <name>Client Library for SOAP with Axis Stubs</name>
  <description>Client library for SOAP including stub codes generated by Apache Axis for the Service Grid Server Software and java web services.</description>
  <url>http://langrid.org/</url>
  <licenses>
    <license>
      <name>GNU LESSER GENERAL PUBLIC LICENSE</name>
      <url>http://www.gnu.org/licenses/lgpl-2.1.txt</url>
    </license>
  </licenses>
  <build>
    <plugins>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>build-helper-maven-plugin</artifactId>
        <version>1.9.1</version>
        <executions>
          <execution>
            <id>add-source</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>add-source</goal>
            </goals>
            <configuration>
              <sources>
                <source>src/stubs</source>
              </sources>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <parent>
    <groupId>org.langrid</groupId>
    <artifactId>servicegrid</artifactId>
    <version>1.1.3</version>
  </parent>
  <dependencies>
  	<dependency>
  		<groupId>junit</groupId>
  		<artifactId>junit</artifactId>
  		<scope>test</scope>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.commons</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.client</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.client.axis</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.client.soap</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.language</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.service.common_1_2</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  	<dependency>
  		<groupId>org.langrid</groupId>
  		<artifactId>jp.go.nict.langrid.service.language_1_2</artifactId>
  		<version>1.1.3</version>
  	</dependency>
  </dependencies>
</project>
